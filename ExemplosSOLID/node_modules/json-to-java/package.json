  "main": "bin/index.js",
  "types": "bin/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/ColinTree/json-to-java"
  },
  "bugs": {
    "url": "https://github.com/ColinTree/json-to-java/issues"
  },
  "homepage": "https://github.com/ColinTree/json-to-java#readme",
  "author": "colintree",
  "license": "Apache-2.0",
  "dependencies": {
    "command-line-args": "^5.1.1",
    "fs-extra": "^8.1.0",
    "lodash": "^4.17.15"
  },
  "devDependencies": {
    "@types/command-line-args": "^5.0.0",
    "@types/fs-extra": "^8.0.0",
    "@types/lodash": "^4.14.140",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "typescript": "^3.6.3"
  }
}
